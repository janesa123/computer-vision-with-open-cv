import cv2
import numpy as np
from google.colab.patches import cv2_imshow
image = cv2.imread('/content/cv exp 1.d.png')
if image is not None:
    gray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)
    blurred = cv2.GaussianBlur(gray, (5, 5), 0)
    unsharp_mask = cv2.subtract(gray, blurred)
    k = 2.0  # High-boost coefficient (k > 1 for high-boost)
    high_boost_image = cv2.addWeighted(gray, 1 + k, blurred, -k, 0)
    cv2_imshow(image)
    cv2_imshow(high_boost_image)
else: print("Error: Could not load image.")
